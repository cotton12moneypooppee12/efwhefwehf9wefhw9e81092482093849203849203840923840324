local ADMINS = {
    3251732187, -- ccvtj
    1, -- qowd

}
local MODS = {
    2, -- qowd
    3, -- ccvtj
}
local STARS = {
    4, -- qowd
    5, -- ccvtj

}
local BOOSTERS = {
    6, -- qowd
    7, -- ccvtj

}
local NOEMOJI = {
    8, -- XD

}
local MONEYNAME = {
    9, -- XD
